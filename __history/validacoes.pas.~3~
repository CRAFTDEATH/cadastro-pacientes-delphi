  function validarCpf(ACpf: integer): string;
implementation
  function validarCpf(ACpf: integer): string;
  {var
     Lista : TObjectList<TStringItemList>;}
  begin
       {if ACpf.IsEmpty then
       begin
         Lista.Add('Cpf Vazio');
       end;}
       Result := InttoStr(ACpf);


  end;
end.
